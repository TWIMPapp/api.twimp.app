{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": "",
  "devCommand": "",
  "rewrites": [
    { "source": "/play", "destination": "/api/play" },
    { "source": "/awty", "destination": "/api/awty" },
    { "source": "/next", "destination": "/api/next" },
    { "source": "/list", "destination": "/api/list" },
    { "source": "/easter_event/:path*", "destination": "/api/easter_event/:path*" },
    { "source": "/custom-trail/create", "destination": "/api/custom-trail/create" },
{ "source": "/custom-trail/:id", "destination": "/api/custom-trail/[id]" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, baggage, sentry-trace" }
      ]
    }
  ]
}
